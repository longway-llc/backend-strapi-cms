{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "name": "Order"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "status": {
      "type": "enumeration",
      "enum": [
        "open",
        "inProgress",
        "paid",
        "closed"
      ],
      "default": "open"
    },
    "saleProductData": {
      "type": "component",
      "repeatable": true,
      "component": "sale-product-data.sale-product-data"
    },
    "user": {
      "via": "orders",
      "plugin": "users-permissions",
      "model": "user"
    }
  }
}
